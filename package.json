    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-modal": "^1.7.7",
    "react-router": "^3.0.5",
    "react-router-dom": "^4.1.1",
    "react-scrollchor": "^3.0.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
